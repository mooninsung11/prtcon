    "version": "0.2.0",
    "configurations": [
      
        
        {
            "name": "Python 디버거: Flask",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true,
            "autoStartBrowser": false
        },
    {
      "name": "Python 디버거: Flask (app1.py)",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "app1.py",
        "FLASK_DEBUG": "1"
      },
      "args": [
        "run",
        "--port=5001",
        "--no-debugger",
        "--no-reload"
      ],
      "jinja": true,
      "autoStartBrowser": false
    }
  ]
}